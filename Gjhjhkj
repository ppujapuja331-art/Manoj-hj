    <script>
        // --- CONFIG ---
        const firebaseConfig = {
            apiKey: "AIzaSyCaNFZqRqZKlhdKp9p4LGsnGwBok5x6f6g",
            databaseURL: "https://manoj-photo-aafec-default-rtdb.firebaseio.com",
            projectId: "manoj-photo-aafec"
        };
        firebase.initializeApp(firebaseConfig);
        const db = firebase.database();

        let me = null; let tempImg = ""; let photoSet = false; let bLink = "#";

        function showMsg(t) { let b = document.getElementById('toast'); b.innerText = t; b.style.display = 'block'; setTimeout(() => b.style.display = 'none', 3000); }

        function readImg(input, target) {
            const r = new FileReader();
            r.onload = e => {
                document.getElementById(target).src = e.target.result;
                document.getElementById(target).style.display = 'block';
                if(target === 'dp-prev') photoSet = true;
                if(target === 'up-prev-img') tempImg = e.target.result;
            };
            r.readAsDataURL(input.files[0]);
        }

        function register() {
            let n = document.getElementById('v-name').value, g = document.getElementById('v-mail').value.trim().toLowerCase(), p = document.getElementById('v-pass').value, d = document.getElementById('dp-prev').src;
            if(!n || !g || !p || !photoSet) return showMsg("‚ùå ‡§™‡•Ç‡§∞‡•Ä ‡§ú‡§æ‡§®‡§ï‡§æ‡§∞‡•Ä ‡§≠‡§∞‡•á‡§Ç!");
            
            showMsg("‚è≥ ‡§≤‡•â‡§ó‡§ø‡§® ‡§π‡•ã ‡§∞‡§π‡§æ ‡§π‡•à...");
            let uid = "u" + g.replace(/[.#$[\]]/g, "_");
            db.ref('users/' + uid).once('value', s => {
                if(s.exists() && s.val().password !== p) return showMsg("‚ùå ‡§ó‡§≤‡§§ ‡§™‡§æ‡§∏‡§µ‡§∞‡•ç‡§°!");
                if(!s.exists()) db.ref('users/' + uid).set({ uid, name: n, gmail: g, password: p, dp: d, balance: 10, followers: 0, postCount: 0, monDone: false });
                localStorage.setItem('vip_uid', uid);
                location.reload();
            });
        }

        function showScr(id) {
            document.querySelectorAll('.scr').forEach(s => s.classList.remove('active'));
            document.getElementById('scr-' + id).classList.add('active');
            if(id === 'msg') document.getElementById('msg-badge').style.display = 'none';
        }

        function sync() {
            if(me.password === "9754560120") document.getElementById('adm-box').style.display = 'block';
            db.ref('settings/banner').on('value', s => { let d = s.val(); if(d){ document.getElementById('vip-banner').style.backgroundImage = `url('${d.img}')`; bLink = d.url; } });
            
            db.ref('users/' + me.uid).on('value', s => {
                me = s.val();
                if(!me) return;
                document.getElementById('my-dp-img').src = me.dp;
                document.getElementById('my-dp-img-p').src = me.dp;
                document.getElementById('u-bal').innerText = "‚Çπ" + (me.balance || 0);
                document.getElementById('u-foll').innerText = (me.followers || 0);
                document.getElementById('my-b-count').innerText = "‚Çπ" + (me.balance || 0);
                document.getElementById('my-f-count').innerText = (me.followers || 0);
                document.getElementById('my-p-count').innerText = (me.postCount || 0);
                if(me.followers >= 5000 && !me.monDone) document.getElementById('mithai-btn').style.display = 'block';
            });

            db.ref('alerts/' + me.uid).on('value', s => {
                let h = ""; let count = 0;
                s.forEach(c => { count++; let n = c.val(); h = `<div class="notif-item"><img src="${n.dp}"><span><b>${n.name}</b> ‡§®‡•á ‡§∏‡§Ç‡§¶‡•á‡§∂ ‡§≠‡•á‡§ú‡§æ: "${n.msg}"</span></div>` + h; });
                document.getElementById('notif-list').innerHTML = h || "<p style='margin:20px; color:#666;'>‡§ï‡•ã‡§à ‡§∏‡§Ç‡§¶‡•á‡§∂ ‡§®‡§π‡•Ä‡§Ç ‡§π‡•à‡•§</p>";
                if(count > 0 && !document.getElementById('scr-msg').classList.contains('active')) document.getElementById('msg-badge').style.display = 'block';
            });

            db.ref('posts').on('value', s => {
                let h = ""; let myGrid = "";
                s.forEach(child => {
                    let p = child.val();g
                    h = `<div class="post">
                        <div class="p-head"><img src="${p.dp}" onclick="openUser('${p.uid}')"><div class="p-head-info" onclick="openUser('${p.uid}')"><b>${p.name}</b><span id="stat-${p.pid}">Report...</span></div><button class="btn-follow" onclick="follow('${
